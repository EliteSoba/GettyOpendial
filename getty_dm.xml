<?xml version="1.0" encoding="UTF-8"?>
<domain>
	<!-- Dialogue Management -->
	<model trigger="NameOfCultureStatus,NameOfArtistStatus">
		<rule>
			<case>
				<condition>
					<if var="current_step" value="NameOfCulture" />
				</condition>
				<condition>
					<if var="NameOfCultureStatus" value="filled" />
				</condition>
				<effect util="5">
					<set var="u_m" value="Culture set to {NameOfCulture}." />
				</effect>
			</case>
		</rule>
	</model>
</domain>